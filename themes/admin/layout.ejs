<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title><%= title %></title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.6 -->
  <link rel="stylesheet" href="<%=require('assets').get('adminTheme')%>/bootstrap/css/bootstrap.min.css">
  
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  
  <!-- jvectormap -->
  <link rel="stylesheet" href="<%=require('assets').get('adminTheme')%>/plugins/jvectormap/jquery-jvectormap-1.2.2.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="<%=require('assets').get('adminTheme')%>/dist/css/AdminLTE.min.css">
  <!-- AdminLTE Skins. Choose a skin from the css/skins
       folder instead of downloading all of them to reduce the load. -->
  <link rel="stylesheet" href="<%=require('assets').get('adminTheme')%>/dist/css/skins/_all-skins.min.css">

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->

  <link rel="stylesheet" href="<%=require('assets').get('adminTheme')%>/css/retalapp.css">
  <link href="<%=require('assets').get('adminTheme')%>/plugins/prism/prism.css" rel="stylesheet" />
</head>
<body class="hold-transition skin-black sidebar-mini">
<div class="wrapper">

  <header class="main-header">

    <!-- Logo -->
    <a href="index2.html" class="logo">
      <!-- mini logo for sidebar mini 50x50 pixels -->
      <span class="logo-mini"><b>Y</b>PJ</span>
      <!-- logo for regular state and mobile devices -->
      <span class="logo-lg"><b>Your</b>PROJECT</span>
    </a>

    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top">
      <!-- Sidebar toggle button-->
      <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
        <span class="sr-only">Toggle navigation</span>
      </a>
      <h1><%= title %></h1>
      <div class="navbar-custom-menu">
        <ul class="nav navbar-nav">
          <li class="dropdown user user-menu">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <img src="<%=require('assets').get('adminTheme')%>/dist/img/user2-160x160.jpg" class="user-image" alt="User Image">
              <span class="hidden-xs">Alexander Pierce</span>
            </a>
            <ul class="dropdown-menu">
              <!-- User image -->
              <li class="user-header">
                <img src="<%=require('assets').get('adminTheme')%>/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">

                <p>
                  Alexander Pierce - Web Developer
                  <small>Member since Nov. 2012</small>
                </p>
              </li>
              <li class="user-footer">
                <div class="pull-left">
                  <a href="#" class="btn btn-default btn-flat"><%=require('lang').t('Profile')%></a>
                </div>
                <div class="pull-right">
                  <a href="#" class="btn btn-default btn-flat"><%=require('lang').t('Sign out')%></a>
                </div>
              </li>
            </ul>
          </li>
        </ul>
      </div>

    </nav>
  </header>

  <aside class="main-sidebar">
    <section class="sidebar">
      <ul class="sidebar-menu">
        <%-require('admin').showWillMenu()%>
      </ul>
    </section>
  </aside>

  <div class="content-wrapper">
    <%-require('admin').showWillMessages()%>
    <%- body %>
    
  </div>
</div>

<script src="<%=require('assets').get('adminTheme')%>/plugins/jQuery/jquery-2.2.3.min.js"></script>
<script src="<%=require('assets').get('adminTheme')%>/bootstrap/js/bootstrap.min.js"></script>
<script src="<%=require('assets').get('adminTheme')%>/plugins/fastclick/fastclick.js"></script>
<script src="<%=require('assets').get('adminTheme')%>/dist/js/app.min.js"></script>
<script src="<%=require('assets').get('adminTheme')%>/plugins/sparkline/jquery.sparkline.min.js"></script>
<script src="<%=require('assets').get('adminTheme')%>/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
<script src="<%=require('assets').get('adminTheme')%>/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
<script src="<%=require('assets').get('adminTheme')%>/plugins/slimScroll/jquery.slimscroll.min.js"></script>
<script src="<%=require('assets').get('adminTheme')%>/plugins/chartjs/Chart.min.js"></script>
<script src="<%=require('assets').get('adminTheme')%>/dist/js/demo.js"></script>
<script src="<%=require('assets').get('adminTheme')%>/plugins/prism/prism.js"></script>
</body>
</html>
